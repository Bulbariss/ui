    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-offline": "3.2.8",
    "gatsby-plugin-postcss": "2.3.3",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-sharp": "2.6.10",
    "gatsby-plugin-styled-jsx": "^3.3.5",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-sharp": "^2.5.4",
    "postcss-100vh-fix": "^0.1.1",
    "prop-types": "15.7.2",
    "pts": "^0.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-pts-canvas": "^0.2.8",
    "simplex-noise": "^2.4.0",
    "styled-jsx": "^3.3.0"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "storybook2": "NODE_ENV=production start-storybook -p 6006 -s ./public",
    "build-storybook": "NODE_ENV=production build-storybook -s ./public"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "storybook-addon-performance": "^0.11.0",
    "tailwindcss": "1.4.6"
  }
}
