  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "dependencies": {
    "@bulbariss/ui": "^1.0.9",
    "babel-preset-react-app": "^9.1.2",
    "framer-motion": "^1.11.0",
