    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.33",
    "gatsby-plugin-styled-jsx": "^3.3.11",
    "gatsby-source-filesystem": "^2.3.28",
    "gatsby-transformer-sharp": "^2.5.14",
    "postcss-100vh-fix": "^0.1.1",
    "prop-types": "15.7.2",
    "pts": "^0.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-pts-canvas": "^0.2.8",
    "simplex-noise": "^2.4.0",
    "styled-jsx": "^3.3.0",
    "tailwind": "^4.0.0"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "storybook2": "NODE_ENV=production start-storybook -p 6006 -s ./public",
    "build-storybook": "NODE_ENV=production build-storybook -s ./public"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.1.1",
    "storybook-addon-performance": "^0.12.0",
    "tailwindcss": "1.8.5"
  }
